{
	"require": {
		"craftcms/ckeditor": "^4.0.3",
		"craftcms/cms": "^5.0.0",
		"doublesecretagency/craft-cpcss": "^3.0.0",
		"nystudio107/craft-minify": "^5.0.0-beta.2",
		"nystudio107/craft-seomatic": "^5.0.0-beta.8",
		"nystudio107/craft-vite": "^5.0.0-beta.3",
		"vlucas/phpdotenv": "^5.4.0"
	},
	"require-dev": {
		"nystudio107/craft-autocomplete": "^1.10",
		"yiisoft/yii2-shell": "^2.0.3"
	},
	"autoload": {
		"psr-4": {
			"modules\\": "modules/"
		}
	},
	"config": {
		"allow-plugins": {
			"craftcms/plugin-installer": true,
			"yiisoft/yii2-composer": true
		},
		"sort-packages": true,
		"optimize-autoloader": true,
		"platform": {
			"php": "8.2"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"craft-update": [
			"@pre-craft-update",
			"@post-craft-update"
		],
		"pre-craft-update": [],
		"post-craft-update": [
			"@php craft install/check && php craft up --interactive=0 || exit 0",
			"@php craft install/check && php craft clear-caches/all --interactive=0 || exit 0",
			"@php craft install/check && php craft invalidate-tags/all --interactive=0 || return 0"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php craft setup/welcome"
		],
		"pre-update-cmd": "@pre-craft-update",
		"pre-install-cmd": "@pre-craft-update",
		"post-update-cmd": "@post-craft-update",
		"post-install-cmd": "@post-craft-update",
		"post-pull-dev": [
			"@php craft install/check && php craft clear-caches/all || return 0",
			"@php craft install/check && php craft migrate/all || return 0",
			"@php craft install/check && php craft project-config/apply || return 0",
			"@php craft install/check && php craft migrate/all || return 0"
		]
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.craftcms.com",
			"canonical": false
		}
	]
}
